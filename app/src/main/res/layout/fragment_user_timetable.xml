<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusableInTouchMode="true"
    tools:context="de.myplan.android.UserTimetable$PlaceholderFragment">

    <TextView
        android:id="@+id/tt_textView_teacher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="@string/teacher"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tt_toggleButton_editsave"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tt_textView_subject"
        app:layout_constraintTop_toTopOf="@+id/tt_textView_subject" />

    <TextView
        android:id="@+id/tt_textView_subject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
