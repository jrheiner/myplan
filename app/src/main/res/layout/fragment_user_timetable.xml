        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tt_textView_teacher"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tt_textView_hour"
        app:layout_constraintTop_toTopOf="@+id/tt_textView_hour" />

    <ToggleButton
        android:id="@+id/tt_toggleButton_editsave"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:checked="false"
        android:textOff="@string/togglebtn_edit"
        android:textOn="@string/togglebtn_save"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tt_textView_hour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/hour"
        android:textAllCaps="false"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/tt_toggleButton_editsave"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view"
